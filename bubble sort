def bubble_sort(lst):
    for i in range(len(lst)-1,0,-1):
        for j in range(i):
            if lst[j] > lst[j+1}:
                temp = lst[j]
                lst[j] = lst[j+1]
                lst[j+1] = temp
                print(lst)
                
lst = [2,4,8,3,5,7,9,22,5,44,10]
sorted = bubble_sort(lst)
print(sorted)
